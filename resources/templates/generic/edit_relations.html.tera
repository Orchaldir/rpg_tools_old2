{% extends "base" %}

{% block content %}
<div class="left_right_container">
  <p><h1>Edit {{ title }} of {{ name }}</h1></p>
  <p><h2>Delete</h2></p>
  <div class="text">
    <ul>
      {% for r in relations %}
      <li><a href="/character/details/{{ r.0 }}">{{ r.1 }}</a>: {{ r.2 }}</li>
      {% endfor %}
    </ul>
    <p><a href="/character/details/{{ id }}">Back</a></p>
  </div>
</div>
{% endblock content %}
